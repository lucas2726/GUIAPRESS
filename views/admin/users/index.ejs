<%-include('../../partials/header.ejs')%>
<%-include('../../partials/navbar.ejs')%>
<body>
    <div class="container">
     <hr>
     <h2>Usuários</h2>
     <a class="btn btn-success" href="/admin/users/create">Criar nova categoria</a>
     <hr>
     <table class="table table-bordered">
         <thead>
             <tr>
                 <th>ID</th>
                 <th>Email</th>
             </tr>
         </thead>
         <tbody>
             <% users.forEach(user => { %> <!--Serve com um for para os conteúdo do BD-->
                 <tr>
                    <td><%= user.id %></td>
                    <td><%= user.email %></td>
                 </tr>
             <% }) %>
         </tbody>
     </table>
    </div> 
 </body>
<%-include('../../partials/footer.ejs')%>